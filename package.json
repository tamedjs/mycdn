{
  "name": "@tamedjs/mycdn",
  "version": "0.1.0",
  "description": "A local development server to work on static files, REST endpoints, reverse proxy, and caching proxied responses.",
  "main": "index.js",
  "keywords": [],
  "author": "The TRG, LLC",
  "license": "MIT",
  "config": {
    "port": 5000
  },
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**"
  },
  "scripts": {
    "app": "",
    "app:build": "npx parcel build ./src/js/browser/index.html --no-cache ",
    "app:server": "node src/js/server/index.js",
    "app:server:watch": "nodemon --exec \"reset; yarn run app:server\" --watch ./src/js/server",
    "app:sign:watch": "npx nodemon --exec \"node ./src/js/server/sign.js\" --watch ./src/js/server",
    "app:watch": "npx parcel watch ./src/js/browser/index.html --no-cache ",
    "start": "npm run app",
    "test": "jest",
    "test:watch": "jest --watch --coverage"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "fs-extra": "^9.0.1",
    "http-proxy": "^1.18.1",
    "json5": "^2.1.3",
    "kill-port": "^1.6.1",
    "live-server": "^1.2.1",
    "node-fetch": "^2.6.1",
    "pem": "^1.14.4",
    "redbird": "^0.10.0",
    "selfsigned": "^1.10.8"
  },
  "devDependencies": {
    "jest": "^26.5.3",
    "nodemon": "^2.0.5",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.5.0",
    "sirloin": "^0.17.3"
  }
}
