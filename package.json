{
  "name": "@tamedjs/mycdn",
  "version": "0.1.0",
  "description": "A local development server to work on static files, REST endpoints, reverse proxy, and caching proxied responses.",
  "keywords": [],
  "author": "The TRG, LLC",
  "license": "MIT",
  "config": {
    "port": 5000
  },
  "staticFiles": {
    "staticPath": "public",
    "watcherGlob": "**"
  },
  "scripts": {
    "api": "npx nodemon --exec \"node dist/server/api.js\" --watch dist/server/api.js ",
    "api:dev": "npx microbundle watch --entry src/js/server/api/start.js --output dist/server/api.js --format cjs",
    "app": "node src/js/server/index.js",
    "app:dev": "npx parcel watch ./src/js/browser/index.html --no-cache ",
    "proxy": "npx nodemon --exec \"node dist/server/proxy.js\" --watch dist/server/proxy.js ",
    "proxy:build": "npx microbundle build --entry src/js/server/proxy.js --output dist/server/proxy.js",
    "proxy:dev": "npx microbundle watch --entry src/js/server/proxy/start.js --output dist/server/proxy.js --format cjs"
  },
  "dependencies": {
    "deepmerge": "^4.2.2",
    "fs-extra": "^9.0.1",
    "json5": "^2.1.3",
    "kill-port": "^1.6.1",
    "live-server": "^1.2.1",
    "nanoid": "^3.1.20",
    "nodemon": "^2.0.6",
    "redbird": "^0.10.0",
    "sirloin": "^0.18.1",
    "voca": "^1.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "microbundle": "^0.12.4",
    "parcel-bundler": "^1.12.4",
    "parcel-plugin-static-files-copy": "^2.5.0"
  }
}
